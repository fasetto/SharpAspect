<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net6.0</TargetFramework>

    <PackageId>SharpAspect</PackageId>
    <Version>4.1.2</Version>
    <Title>SharpAspect: AOP implementation for .Net</Title>
    <Description>AOP implementation for .Net</Description>
    <PackageReleaseNotes>A changelog is available at https://github.com/fasetto/SharpAspect/blob/master/CHANGELOG.md.</PackageReleaseNotes>
    <Authors>fasetto</Authors>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
    <PackageOutputPath>../../.nuget/</PackageOutputPath>
    <PackageTags>aspect;sharp-aspect;aop;dynamic-proxy</PackageTags>
    <PackageProjectUrl>https://github.com/fasetto/SharpAspect</PackageProjectUrl>
    <RepositoryUrl>https://github.com/fasetto/SharpAspect</RepositoryUrl>
    <DocumentationUrl>https://raw.githubusercontent.com/fasetto/SharpAspect/master/README.md</DocumentationUrl>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Castle.Core" Version="4.4.1" />
    <PackageReference Include="Castle.Core.AsyncInterceptor" Version="2.0.0" />
    <PackageReference Include="Microsoft.Extensions.DependencyInjection" Version="6.0.0" />
  </ItemGroup>

</Project>
